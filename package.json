{
  "name": "v-json-tree",
  "description": "Vue.js 3.0 JSON tree viewer",
  "version": "3.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikemenaker/vue-json-tree.git"
  },
  "author": "Mike Menaker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikemenaker/vue-json-tree/issues"
  },
  "homepage": "https://github.com/mikemenaker/vue-json-tree#readme",
  "main": "./dist/vJsonTree.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vJsonTree ./src/components/JsonTree.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
